    <script src="//cdn.bootcss.com/ckeditor/4.5.9/plugins/justify/plugin.js"></script>
    <script src="//cdn.bootcss.com/ckeditor/4.5.9/plugins/justify/lang/zh-cn.js"></script>
    <script src="//cdn.bootcss.com/ckeditor/4.5.9/plugins/embed/plugin.js"></script>
    <script src="//cdn.bootcss.com/ckeditor/4.5.9/plugins/embedbase/plugin.js"></script>
    <script src="//cdn.bootcss.com/ckeditor/4.5.9/plugins/embedbase/dialogs/embedbase.js"></script>
    <script src="//cdn.bootcss.com/ckeditor/4.5.9/plugins/embedbase/lang/zh-cn.js"></script>

    <script src="~/Content/js/ckeditor.js"></script>

    <script src="~/Content/js/common.js"></script>
    @RenderSection("scripts", required: false)
</head>
<body>
    @RenderBody()

    @*<script src="//cdn.ckeditor.com/4.5.9/standard/ckeditor.js"></script>*@

</body>

</html>