<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link href="~/Content/themes/bootstrap/easyui.css" rel="stylesheet" />
    <link href="~/Content/themes/icon.css" rel="stylesheet" />
    <link href="~/Content/css/site.admin.css" rel="stylesheet" />
    <link href="//cdn.bootcss.com/ckeditor/4.5.9/contents.css" rel="stylesheet">
    <script src="~/Scripts/jquery-1.11.3.min.js"></script>
    <script src="~/Scripts/jquery.easyui-1.4.3.min.js"></script>
    <script src="~/Scripts/locale/easyui-lang-zh_CN.js"></script>
    <script src="//cdn.bootcss.com/ckeditor/4.5.9/ckeditor.js"></script>
    <script src="//cdn.bootcss.com/ckeditor/4.5.9/config.js"></script>
    <script src="//cdn.bootcss.com/ckeditor/4.5.9/lang/zh.js"></script>
    <script src="//cdn.bootcss.com/ckeditor/4.5.9/plugins/flash/dialogs/flash.js"></script>
    <script src="//cdn.bootcss.com/ckeditor/4.5.9/plugins/flash/plugin.js"></script>
    <script src="//cdn.bootcss.com/ckeditor/4.5.9/plugins/flash/lang/zh.js"></script>


    <script src="//cdn.bootcss.com/ckeditor/4.5.9/plugins/colorbutton/plugin.js"></script>
    <script src="//cdn.bootcss.com/ckeditor/4.5.9/plugins/colorbutton/lang/zh-cn.js"></script>

    <script src="//cdn.bootcss.com/ckeditor/4.5.9/plugins/colordialog/dialogs/colordialog.js"></script>
