@using MedQC.Web;
@{
    string system = ViewBag.system != null ? (ViewBag.system as string) : MedQC.Web.Utility.CookieHelper.GetCookie("system") as string;

    if (string.IsNullOrEmpty(system))
    {
        system = "医疗质量管理平台";
    }
    else
    {
        system = System.Web.HttpUtility.UrlDecode(system, System.Text.Encoding.UTF8);
    }
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta content="@ViewBag.Keywords" name="keywords">
    <meta content="@ViewBag.Description" name="description">
    <meta name="author" content="@ViewBag.Author" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
   
    <meta name="viewport"
          content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <meta property="qc:admins" content="41333617676214416761676375" />
    <title>@ViewBag.Title</title>
    <link rel="icon" href="/logo.ico" type="image/x-icon" />
    <link rel="shortcut icon" href="/logo.ico" type="image/x-icon" />
    <link href="~/Scripts/AmazeUI/assets/css/amazeui.min.css" rel="stylesheet" />
   
    <link href="~/Content/css/site.medqc.css" rel="stylesheet" />

    <script src="~/Scripts/jquery-1.11.3.min.js"></script>
    <script src="//cdn.bootcss.com/jquery.lazyload/1.9.1/jquery.lazyload.min.js"></script>
    <script src="~/Scripts/jquery.scrollbox.js"></script>
    <script src="//cdn.bootcss.com/jquery.form/3.51/jquery.form.min.js"></script>
    <script src="//cdn.bootcss.com/jsSHA/2.1.0/sha1.js"></script>
   



    <script src="~/Scripts/echarts.min.js"></script>
    <link href="~/Scripts/easyui/themes/bootstrap/easyui.css" rel="stylesheet" />
    <link href="~/Scripts/easyui/themes/icon.css" rel="stylesheet" />
    <link href="~/Scripts/easyui/themes/color.css" rel="stylesheet" />
    <script src="~/Scripts/easyui/jquery.easyui.min.js"></script>
    <script src="~/Scripts/easyui/locale/easyui-lang-zh_CN.js"></script>

    <!--百度ueditor-->
    <script type="text/javascript" charset="utf-8" src="~/Scripts/ueditor/ueditor.config.js"></script>
    <script type="text/javascript" charset="utf-8" src="~/Scripts/ueditor/ueditor.all.js"> </script>
    <!--建议手动加在语言，避免在ie下有时因为加载语言失败导致编辑器加载失败-->
    <!--这里加载的语言文件会覆盖你在配置项目里添加的语言类型，比如你在配置项目里配置的是英文，这里加载的中文，那最后就是中文-->
    <script type="text/javascript" charset="utf-8" src="~/Scripts/ueditor/lang/zh-cn/zh-cn.js"></script>
   
</head>

<body>

    <!--header begin-->

    @Html.Partial("_Header")

    <!--header end-->
   

    <div class="am-cf admin-main">
        <!-- sidebar start -->
        @if (system == "医生辅助学习系统")
        {
            @Html.Partial("_SideBar2")
        }
        else
        {
            @Html.Partial("_SideBar")
        }
        <!-- sidebar end -->
        <!-- content start -->
        @RenderBody()
        <!-- content end -->
    </div>
    <!--footer begin-->
   
    <!--footer end-->

    <script src="~/Scripts/AmazeUI/assets/js/amazeui.min.js"></script>
    <script src="~/Content/js/medqc.js"></script>
    <script src="~/Content/js/ueditor.js"></script>
</body>

</html>
