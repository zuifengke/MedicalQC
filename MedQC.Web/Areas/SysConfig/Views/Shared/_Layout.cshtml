<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link href="~/Content/themes/bootstrap/easyui.css" rel="stylesheet" />
    <link href="~/Content/themes/icon.css" rel="stylesheet" />
    <link href="~/Content/css/site.admin.css" rel="stylesheet" />
    <link href="//cdn.bootcss.com/ckeditor/4.5.9/contents.css" rel="stylesheet">
    <script src="~/Scripts/jquery-easyui-1.5/jquery.min.js"></script>
    <script src="~/Scripts/jquery-easyui-1.5/plugins/jquery.form.js"></script>
    <script src="~/Scripts/jquery-easyui-1.5/jquery.easyui.min.js"></script>
    <script src="~/Scripts/locale/easyui-lang-zh_CN.js"></script>
    <script src="//cdn.bootcss.com/ckeditor/4.5.9/ckeditor.js"></script>
    <script src="//cdn.bootcss.com/ckeditor/4.5.9/config.js"></script>
    <script src="//cdn.bootcss.com/ckeditor/4.5.9/lang/zh.js"></script>
    <script src="//cdn.bootcss.com/ckeditor/4.5.9/plugins/flash/dialogs/flash.js"></script>
    <script src="//cdn.bootcss.com/ckeditor/4.5.9/plugins/flash/plugin.js"></script>
    <script src="//cdn.bootcss.com/ckeditor/4.5.9/plugins/flash/lang/zh.js"></script>


    <script src="//cdn.bootcss.com/ckeditor/4.5.9/plugins/colorbutton/plugin.js"></script>
    <script src="//cdn.bootcss.com/ckeditor/4.5.9/plugins/colorbutton/lang/zh-cn.js"></script>

    <script src="//cdn.bootcss.com/ckeditor/4.5.9/plugins/colordialog/dialogs/colordialog.js"></script>
    <script src="//cdn.bootcss.com/ckeditor/4.5.9/plugins/colordialog/plugin.js"></script>
    <script src="//cdn.bootcss.com/ckeditor/4.5.9/plugins/colordialog/lang/zh-cn.js"></script>
    <script src="//cdn.bootcss.com/ckeditor/4.5.9/plugins/justify/plugin.js"></script>
    <script src="//cdn.bootcss.com/ckeditor/4.5.9/plugins/justify/lang/zh-cn.js"></script>
    <script src="//cdn.bootcss.com/ckeditor/4.5.9/plugins/embed/plugin.js"></script>
    <script src="//cdn.bootcss.com/ckeditor/4.5.9/plugins/embedbase/plugin.js"></script>
    <script src="//cdn.bootcss.com/ckeditor/4.5.9/plugins/embedbase/dialogs/embedbase.js"></script>
    <script src="//cdn.bootcss.com/ckeditor/4.5.9/plugins/embedbase/lang/zh-cn.js"></script>

    <script src="~/Content/js/ckeditor.js"></script>

    <script src="~/Content/js/common.js"></script>
    <script>
        function closes() {
            $("#Loading").fadeOut("normal", function () {
                $(this).remove();
            });
        }
        $(function () {
            setTimeout(closes, 500);
        });
    </script>
    @RenderSection("scripts", required: false)
</head>
<body style="margin:0;">
    <div id='Loading' style="position: absolute; z-index: 1000; top: 0px; left: 0px; width: 100%; height: 100%; background: #fff url('/Content/images/site/grid.png'); text-align: center; padding-top: 20%;">
        <p style="display: inline; font-size: 16px; color: #15428B;">
            <img src="~/Content/images/site/loading10.gif" align="absmiddle" />
        </p>
    </div>
    @RenderBody()


</body>

</html>